<?xml version="1.0"?>
<package >
  <metadata>
    <id>Microsoft.Azure.DCAP.Client</id>
    <version>1.0</version>
    <title>Plugin for use with Intel(R) DCAP to get platform certification info.</title>
    <authors>Microsoft Corporation</authors>
    <owners>Microsoft Corporation</owners>
    <licenseUrl>https://github.com/Microsoft/Azure-DCAP-Client/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Microsoft/Azure-DCAP-Client</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This plugin is intended to be included with SGX-based applications using the Intel(R) SGX DCAP stack (https://github.com/intel/SGXDataCenterAttestationPrimitives). The DCAP stack loads the plugin and uses it to locate certificates and revocation data for SGX platforms.</description>
    <copyright>Copyright ©2018 Microsoft Corporation. All rights reserved.</copyright>

    <dependencies>
      <dependency id="curl" version="7.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="x64\Debug\dcap_quoteprov.dll" target="lib\debug" />
    <file src="x64\Debug\dcap_quoteprov.pdb" target="lib\debug" />
    <file src="x64\Release\dcap_quoteprov.dll" target="lib\release" />
    <file src="x64\Release\dcap_quoteprov.pdb" target="lib\release" />
    <file src="..\dcap_provider.h" target="inc" />
  </files>
</package>
